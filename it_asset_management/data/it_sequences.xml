<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Séquence pour les Tickets -->
    <record id="seq_it_ticket" model="ir.sequence">
        <field name="name">Ticket IT</field>
        <field name="code">it.ticket</field>
        <field name="prefix">TIC-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Séquence pour les Contrats (optionnel) -->
    <record id="seq_it_contract" model="ir.sequence">
        <field name="name">Contrat IT</field>
        <field name="code">it.contract</field>
        <field name="prefix">CON-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
